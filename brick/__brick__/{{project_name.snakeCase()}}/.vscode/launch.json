  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch development",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_development.dart",
      "args": [
        "--flavor",
        "development",
        "--target",
        "lib/main_development.dart"
      ]
    },
    {
      "name": "Launch {{staging_name_full}}",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_{{staging_name_full}}.dart",
      "args": ["--flavor", "{{staging_name_full}}", "--target", "lib/main_{{staging_name_full}}.dart"]
    },
    {
      "name": "Launch production",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_production.dart",
      "args": ["--flavor", "production", "--target", "lib/main_production.dart"]
    }
  ]
}
